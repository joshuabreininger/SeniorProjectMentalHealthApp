<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/changegraphbutton"
        android:layout_width="153dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#ff751a"
        android:text="Mood"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.879"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yearbarchart"
        app:layout_constraintVertical_bias="0.111" />

    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/daybarchart"
        android:layout_width="350dp"
        android:layout_height="110dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/graphsMenuText"
        app:layout_constraintVertical_bias="0.018" />

    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/weekbarchart"
        android:layout_width="350dp"
        android:layout_height="110dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daybarchart"
        app:layout_constraintVertical_bias="0.051" />

    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/monthbarchart"
        android:layout_width="350dp"
        android:layout_height="110dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weekbarchart"
        app:layout_constraintVertical_bias="0.081" />

    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/yearbarchart"
        android:layout_width="350dp"
        android:layout_height="110dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/monthbarchart"
        app:layout_constraintVertical_bias="0.15" />

    <TextView
        android:id="@+id/graphsMenuText"
        android:layout_width="428dp"
        android:layout_height="68dp"
        android:background="#ff3333"
        android:gravity="center_horizontal"
        android:text="Graphs"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#000000"
        android:textSize="44sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="-8dp" />

    <Button
        android:id="@+id/close_scene_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#ff751a"
        android:text="Close"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yearbarchart"
        app:layout_constraintVertical_bias="0.111" />


</androidx.constraintlayout.widget.ConstraintLayout>